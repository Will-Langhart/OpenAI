m_image_markup }}\n          {{ menu_item.title }}\n          {%- if product_count -%}\n            {{ product_count }}\n          {%- endif -%}\n\n          {% if show_arrows and is_mobile != true %}\n            &lt;span\n              class=&quot;navmenu-icon navmenu-icon-depth-{{ depth }}&quot;\n              data-navmenu-trigger\n            &gt;\n              {% render &#x27;icon-chevron-down-small&#x27; %}\n            &lt;/span&gt;\n          {% endif %}\n        {% if is_mobile != true %}\n          &lt;/summary&gt;\n        {% else %}\n          &lt;/a&gt;\n        {% endif %}\n\n        {% if show_arrows and is_mobile == true %}\n          {%\n            render &#x27;mobile-navmenu-button&#x27;,\n            button_label: menu_item.title,\n            depth: depth,\n          %}\n        {% endif %}\n\n        {% unless d &gt; max_depth %}\n          {%\n            render &#x27;navlist&#x27;,\n            menu_list: linklists[menu_item.handle].links,\n            menu_title: menu_title,\n            depth: d,\n            max_depth: max_depth,\n            show_arrows: show_arrows,\n            aria_expanded: aria_expanded,\n            is_mobile: is_mobile,\n            show_prod_count: show_prod_count,\n            accordion_level: accordion_level,\n            show_item_images: show_item_images,\n          %}\n        {% endunless %}\n        {% if is_mobile != true %}\n          &lt;/details&gt;\n        {% endif %}\n      &lt;/li&gt;\n    {% endunless %}\n  {% endfor %}\n&lt;/ul&gt;"]}, "status": "finished_successfully", "end_turn": null, "weight": 1.0, "metadata": {"timestamp_": "absolute", "message_type": null}, "recipient": "all"}, "parent": "b454e99b-363a-4cc2-b162-ae40bff4d5e9", "children": ["b56b68bd-1fd6-4dfd-97bb-fb12f17f6d16"]}, "b56b68bd-1fd6-4dfd-97bb-fb12f17f6d16": {"id": "b56b68bd-1fd6-4dfd-97bb-fb12f17f6d16", "message": {"id": "b56b68bd-1fd6-4dfd-97bb-fb12f17f6d16", "author": {"role": "assistant", "name": null, "metadata": {}}, "crea